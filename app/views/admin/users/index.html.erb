<div class="container">
  <div class="row">
   <h3>会員一覧<i class="fas fa-users"></i></h3>
  </div>

<table class="table cart_items">
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td>
      <%= link_to admin_user_path(user) do %>
        <%= user.last_name %><%= user.first_name %>
      <% end %>
    </td>
    <td><%= user.email %></td>
    <% if user.is_deleted == false %>
      <td>会員</td>
    <% else %>
      <td>退会済み</td>
     <% end %>
  </tr>
  <% end %>
 </table>

</div>